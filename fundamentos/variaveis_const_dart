import 'dart:io';
void main(){
  //variaveis em dart
  
  //dados texto;
  String nome = "marcos";

  //tipos numericos;
  int numero1 =  20;
  double numero2 = 20;

  //tipos de  dado constante;
  //diferença entre final e const ;
  //const
   
  final num = 20;
  const nume = 30; 

  //tipos de dados logicos; 
  bool nomeGrande = true ; 

  //modelo de interpolação de dados em string ; 
  print("meu nome é marcos $nome , idade de $numero2 ");

  //podemos declarar variavel  ultilizando alguns modelos unico de dart 

  dynamic nomes = "marcos"; 

  print(nomes);

  //podemos definir o tipo de avariavel ultilizandoo .to(String) (int)(boll)etc.

  var fone = "3423341";
  fone = 213123.toString();
  
 
 //armazenando dados do  usuario atravez stdin.readlineSync() ?? "0"; 
  print("qual é seu nome:");
  
 String nomeb = stdin.readLineSync() ?? "erro";
 print(nomeb);

 //podemos interpolar ;
  $valor;
  
  valor1.toString();


 



  

